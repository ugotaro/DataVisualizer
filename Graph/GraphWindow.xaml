<Window x:Class="UGO.Utility.GraphWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UGO.Utility"
        xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="GraphWindow" Height="400" Width="600">
    <Grid>
        <oxy:PlotView x:Name="plotView" Margin="10,10,10,41"/>
        <Label Content="{Binding GraphInformation}" Margin="10,0,10,10" VerticalAlignment="Bottom"/>
    </Grid>
</Window>
